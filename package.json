{
  "name": "playyoutube",
  "main": "./lib/playyoutube-atom",
  "version": "1.3.3",
  "description": "Search and play youtube videos inside atom. Follow your favorite tutorials inside the development environment in a floating window",
  "keywords": [
    "youtube",
    "video",
    "player",
    "watch",
    "search"
  ],
  "scripts": {
    "test": "jest",
    "test:dev": "jest --watchAll spec/unit",
    "test:int": "jest test/integration",
    "test:cov": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "activationCommands": {
    "atom-workspace": "playyoutube:search"
  },
  "repository": "https://github.com/Hatisoft/playyoutube-atom",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "event-kit": "^1.5.0",
    "loophole": "^1.1.0",
    "vue": "^1.0.12",
    "youtube-node": "^1.3.0"
  },
  "devDependencies": {
    "coveralls": "3.0.2",
    "jest": "23.4.2"
  },
  "jest": {
    "testMatch": [
      "**/spec/**/*-test.js"
    ],
    "testEnvironment": "node",
    "automock": false
  }
}
